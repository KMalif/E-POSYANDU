                    android:layout_height="wrap_content"
                    android:text="@string/HPHT"
                    android:layout_marginStart="10dp"
                    android:textSize="18sp"
                    />
            </TableRow>

            <TableRow android:layout_marginTop="20dip">

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:text="@string/HamilKe"
                    android:textColor="@color/black"
                    android:textSize="18sp"  />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:text="@string/titikdua"
                    android:textColor="@color/black"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tvHamilKe"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="@string/HamilKe"
                    android:layout_marginStart="10dp"
                    android:textSize="18sp"
                    />
            </TableRow>

            <TableRow android:layout_marginTop="20dip">

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:text="@string/PersalinanKe"
                    android:textColor="@color/black"
                    android:textSize="18sp"  />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:text="@string/titikdua"
                    android:textColor="@color/black"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tvPersalinanKe"
